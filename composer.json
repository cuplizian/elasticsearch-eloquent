{
    "name": "isswp101/elasticsearch-eloquent",
    "type": "library",
    "description": "Elasticsearch functionality like Laravel Eloquent models",
    "keywords": [
        "elasticsearch",
        "eloquent"
    ],
    "homepage": "https://github.com/isswp101/elasticsearch-eloquent",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Sorokin",
            "email": "isswp101@gmail.com",
            "homepage": "https://github.com/isswp101",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0",
        "illuminate/support": "~5.0",
        "shift31/laravel-elasticsearch": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.3.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "vlucas/phpdotenv": "^2.2",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Isswp101\\Persimmon\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Isswp101\\Persimmon\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
